 <%= render partial: "template/page_top",  locals: {guide_message: "積み上げ時間ランキング　#{@goal.name}を目指しているひとたち"}%>
<%= stylesheet_link_tag "book/ranking_page", :media => "all" %>

<div class="message">

</div>
<br>
<div class="ranking_area">
<% @same_goal_users.each.with_index(1) do |(user, total_time), index| %>
  <%= link_to user_path(user.id) ,class: "link" do %>
    <div class="ranking_panel">
    <%= index %>位  <%= user.name%>さん <%= total_time.round(1) %>時間  <br>
    </div>
  <% end %>
<% end %>
</div>
